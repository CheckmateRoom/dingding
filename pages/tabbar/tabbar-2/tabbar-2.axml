<view class="content"><view class="nav"><nav-tab vue-id="1" tabTitle="{{tabTitle}}" ref="__r" data-ref="navTab" data-event-opts="{{[['^changeTab',[['changeTab']]]]}}" onChangeTab="__e" onVueInit="__l"></nav-tab></view><view class="listBody"><swiper style="{{'height:'+(swiperHeight+'px')+';'}}" current="{{currentTab}}" data-event-opts="{{[['change',[['swiperTab',['$event']]]]]}}" onChange="__e"><swiper-item class="list1" style="height:100%;"><block a:if="{{dataSource.unfinishedList&&dataSource.unfinishedList.length>0}}"><scroll-view style="height:100%;" scroll-y="true" scroll-with-animation><block a:for="{{dataSource.unfinishedList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['dataSource.unfinishedList','',index]]]]]]]}}" class="list-item" onTap="__e"><view class="item-header"><text class="title">{{item.title||'-'}}</text></view><view class="item-content"><text class="time">{{"到达时间: "+(item.arriveTime||'-')}}</text><text class="time">{{"完成时间: "+(item.completeTime||'-')}}</text><text class="status">{{"·"+item.currentProgress}}</text></view></view></block></scroll-view></block><block a:else><view class="empty">暂无更多数据</view></block></swiper-item><swiper-item class="list2" style="height:101%;"><block a:if="{{dataSource.finishedList&&dataSource.finishedList.length>0}}"><scroll-view style="height:100%;" scroll-y="true" scroll-with-animation><block a:for="{{dataSource.finishedList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['dataSource.finishedList','',index]]]]]]]}}" class="list-item" onTap="__e"><view class="item-header"><text class="title">{{item.title||'-'}}</text></view><view class="item-content"><text class="time">{{"到达时间: "+(item.arriveTime||'-')}}</text><text class="time">{{"完成时间: "+(item.completeTime||'-')}}</text><text class="status">{{"·"+item.currentProgress}}</text></view></view></block></scroll-view></block><block a:else><view class="empty">暂无更多数据</view></block></swiper-item></swiper></view></view>