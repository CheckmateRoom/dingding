<view class="navTabBox"><block a:if="{{tabTitle.length<=5}}"><view class="shortTab"><view class="navTab"><block a:for="{{tabTitle}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['navClick',[index]]]]]}}" class="{{(('navTabItem')+' '+(index===tabClick?'click':''))}}" onTap="__e">{{item}}</view></block></view><view class="underlineBox" style="{{('transform:translateX('+isLeft+'px);width:'+isWidth+'px')}}"><view class="underline"></view></view></view></block><block a:if="{{tabTitle.length>5}}"><view class="longTab"><scroll-view style="white-space:nowrap;display:flex;" scroll-x="true" scroll-with-animation scroll-into-view="{{toView}}"><block a:for="{{tabTitle}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('longItem')+' '+(index===tabClick?'click':''))}}" data-index="{{index}}" id="{{'id'+index}}" data-event-opts="{{[['tap',[['longClick',[index]]]]]}}" onTap="__e">{{item}}</view></block><view class="underlineBox" style="{{('transform:translateX('+isLeft+'px);')}}"><view class="underline"></view></view></scroll-view></view></block></view>